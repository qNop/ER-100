		include/dictdef.h \
		include/utf16char.h \
		include/searchutility.h \
		include/spellingtrie.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o sync.o share/sync.cpp

userdict.o: share/userdict.cpp include/userdict.h \
		include/atomdictbase.h \
